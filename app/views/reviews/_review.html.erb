<div class="review-rating" data-score="<%= review.rating %>"></div>
<p><%= review.comment%></p>


  <% if user_signed_in? %>
    <% if review.user_id == current_user.id %>
      <%= link_to "edit", edit_ad_review_path(review.ad, review) %>
      <%= link_to "Delete", ad_review_path(review.ad, review), method: :delete, data: { confirm: "are yourik sure?"} %>
    <% end %>
  <% end %>
