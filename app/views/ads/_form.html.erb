<%= simple_form_for @ad do |f| %>
  <%= f.input :title, label: "Naam vervoer" %>
  <%= f.input :categorie %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :brand %>
  <%= f.input :license %>
  <%= f.input :plug %>
  <%= f.input :size %>
  <%= f.input :capacity %>
  <%= f.input :restrictions %>
  <%= f.input :transmission %>
  <%= f.input :year %>
  <%= f.input :engine %>
  <%= f.input :fuel %>
  <%= f.button :submit %>
<% end %>
